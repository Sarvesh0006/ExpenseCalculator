
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Login</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
</head>
<body>
    <div class="col-md-12 row">
        <div class="col-md-4 col-sm-0">

        </div>
        <div class="col-md-4 mt-5 col-sm-12" style="padding-left:23px;">
            <div class="card text-center">
                <div class="card-header">
                    Account
                </div>
                <div class="card-body">
                    <div class="form-group">
                        <div class="col-md-12">
                            <input type="text" id="txt_userid" class="form-control form-control-sm" placeholder="User Name" />
                        </div>
                        <div class="col-md-12 mt-2">
                            <input type="password" id="txt_password" class="form-control form-control-sm" placeholder="Password" />
                        </div>
                    </div>
                </div>
                <div class="card-footer">
                    <button class="btn btn-sm btn-outline-primary" id="btn_login">Login</button>
                </div>
            </div>
        </div>
        <div class="col-md-4">

        </div>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="~/ProjectJS/Account/Login.js" asp-append-version="true"></script>
</body>
</html>
